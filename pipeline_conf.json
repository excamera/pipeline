{
    "daemon_addr": "your-local-ip"
  , "daemon_port": 24680

  , "scheduler": "SimpleScheduler"
  , "_simp_scheduler": "SimpleScheduler"
  , "_con_scheduler": "ConcurrencyLimitPriorityScheduler"
  , "concurrency_limit": 200

  , "default_region": "us-east-1"
  , "storage_base": "s3://lixiang-pipeline/"
  , "hash_bucket": true
  , "temp_storage_base": "s3://pipetemp-"
  , "s3_threadpool_size": 4
  , "straggler_configs": "0.9 2 0.7"
  , "use_tar": false

  , "platform": "aws_lambda"
  , "default_lambda_function": "lambda_test_HWsdxL2M"

  , "aws_access_key_id_file": "path/to/access_key"
  , "aws_secret_access_key_file": "path/to/secret_key"
  , "srvcrt_file": "path/to/server_cert.pem"
  , "srvkey_file": "path/to/server_key.pem"

  , "tracker_port": 13579
  , "tracker_backlog": 3000
}
